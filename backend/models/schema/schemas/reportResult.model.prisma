enum ReporResultState {
    accepted
    dismissed
}

model ReportResult {
    reportId  Int              @id @default(autoincrement())
    createdAt DateTime         @default(now())
    handlerId Int
    result    ReporResultState

    report  Report @relation(fields: [reportId], references: [reportId])
    handler Admin  @relation(fields: [handlerId], references: [adminId])
}
